<p id="notice"><%= notice %></p>

<h1>Listing Foods</h1>

<table>
  <thead>
    <tr>
      <th>Res id</th>
      <th>Cal</th>
      <th>Fat</th>
      <th>Sugar</th>
      <th>Location</th>
      <th>Picture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foods.each do |food| %>
      <tr>
        <td><%= food.res_Id %></td>
        <td><%= food.cal %></td>
        <td><%= food.fat %></td>
        <td><%= food.sugar %></td>
        <td><%= food.location %></td>
        <td><%= food.picture %></td>
        <td><%= link_to 'Show', food %></td>
        <td><%= link_to 'Edit', edit_food_path(food) %></td>
        <td><%= link_to 'Destroy', food, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food', new_food_path %>
